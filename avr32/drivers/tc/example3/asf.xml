<asf xmlversion="1.0">
	<module type="application" id="avr32.drivers.tc.example3" caption="TC Example 3">
		<info type="description" value="summary">
		Set-up a timer/counter and generate a "tick" interrupt each millisecond to toggle a pin. Display the number of elapsed ms (updated every second) on a USART.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Timers"/>
		</info>
		<build type="c-source" value="tc_example3.c"/>
		<build type="module-config" subtype="required-header-file" value="conf_example.h"/>
		<device-support value="uc3a"/>
		<device-support value="uc3a3_a4"/>
		<device-support value="uc3b"/>
		<device-support value="uc3c"/>
		<device-support value="uc3d"/>
		<device-support value="uc3l"/>
		<require idref="common.services.basic.clock"/>
		<require idref="avr32.drivers.gpio"/>
		<require idref="avr32.drivers.tc"/>
		<require idref="avr32.drivers.intc"/>
		<require idref="avr32.utils"/>
		<require idref="avr32.utils.print_funcs"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
