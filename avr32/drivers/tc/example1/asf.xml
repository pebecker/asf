<asf xmlversion="1.0">
	<module type="application" id="avr32.drivers.tc.example1" caption="TC Example 1">
		<info type="description" value="summary">
		Using the TC module, set-up a Timer/Counter to output a 10.16% duty cycle square wave on TIOA0. Switch the device into the FROZEN sleep mode (thus still maintaining the TC activity).
		</info>
		<build type="c-source" value="tc_example1.c"/>
		<device-support value="uc3a"/>
		<device-support value="uc3a3"/>
		<device-support value="uc3b"/>
		<device-support value="uc3c"/>
		<device-support value="uc3d"/>
		<device-support value="uc3l"/>
		<require idref="avr32.drivers.gpio"/>
		<require idref="avr32.drivers.tc"/>
		<require idref="avr32.drivers.pm"/>
		<info type="keyword" value="technology">
			<keyword value="Timers"/>
		</info>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
