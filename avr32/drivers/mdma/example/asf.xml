<asf xmlversion="1.0">
	<module type="application" id="avr32.drivers.mdma.example" caption="MDMA Example">
		<info type="description" value="summary">
		Perform several memory transfers using the MDMA from and to different memory areas: flash memory, external SDRAM, HSB RAM, CPU RAM.
		</info>
		<info type="keyword" value="technology">
			<keyword value="DMA Controller"/>
			<keyword value="Memory"/>
		</info>
		<build type="header-file" value="flashc_buffer.h"/>
		<build type="include-path" value="."/>
		<build type="c-source" value="mdma_example.c"/>
		<device-support value="uc3c"/>
		<require idref="avr32.drivers.cpu.cycle_counter"/>
		<require idref="avr32.drivers.flashc"/>
		<require idref="avr32.drivers.gpio"/>
		<require idref="avr32.drivers.mdma"/>
		<require idref="avr32.drivers.pm"/>
		<require idref="avr32.drivers.scif"/>
		<require idref="avr32.drivers.intc"/>
		<require idref="avr32.components.memory.sdram.mt48lc16m16a2tg7e"/>
		<require idref="avr32.drivers.usart"/>
		<require idref="avr32.utils.print_funcs"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
