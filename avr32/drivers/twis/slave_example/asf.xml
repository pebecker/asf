<asf xmlversion="1.0">
	<module type="application" id="avr32.drivers.twis.slave_example" caption="TWIS Slave Example">
		<info type="description" value="summary">
		Using the TWIS module, process read and write operations from/to a virtual memory space pre-allocated in internal RAM. To be used with two boards with the corresponding TWIM module example.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Communication"/>
		</info>
		<build type="c-source" value="twis_example.c"/>
		<device-support value="uc3a3_a4"/>
		<device-support value="uc3c"/>
		<device-support value="uc3l"/>
		<device-support value="uc3d"/>
		<require idref="avr32.drivers.gpio"/>
		<require idref="avr32.drivers.pm"/>
		<require idref="avr32.drivers.twis"/>
		<require idref="avr32.utils.print_funcs"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
