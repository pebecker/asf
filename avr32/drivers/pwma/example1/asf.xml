<asf xmlversion="1.0">
	<module type="application" id="avr32.drivers.pwma.example1" caption="PWMA Example 1">
		<info type="description" value="summary">
		Using PWMA module, generating wavefrom to two different channels and updating
		the duty cycle values using Interlinked multi-value mode. Timebase overflow 
		interrupt is enabled which gets triggered whenever overflow occurs.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Clocks"/>
		</info>
		<build type="c-source" value="pwma_example1.c"/>
		<device-support value="uc3l"/>
		<device-support value="uc3d"/>
		<require idref="common.services.basic.clock"/>
		<require idref="common.services.delay"/>
		<require idref="avr32.drivers.pwma"/>
		<require idref="avr32.drivers.gpio"/>
		<require idref="avr32.drivers.intc"/>
		<require idref="avr32.drivers.pm"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
