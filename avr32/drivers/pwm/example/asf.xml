<asf xmlversion="1.0">
	<module type="application" id="avr32.drivers.pwm.example" caption="PWM Example">
		<info type="description" value="summary">
		Output a periodic(22.5Hz) waveform on pin using the PWM module.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Clocks"/>
		</info>
		<build type="c-source" value="pwm_example.c"/>
		<device-support value="uc3a"/>
		<device-support value="uc3b"/>
		<require idref="avr32.drivers.gpio"/>
		<require idref="avr32.drivers.intc"/>
		<require idref="avr32.drivers.pwm"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
