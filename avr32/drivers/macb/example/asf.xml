<asf xmlversion="1.0">
	<module type="application" id="avr32.drivers.macb.example" caption="MACB Example">
		<info type="description" value="summary">
		Reply to ping requests on a local network (low-level implementation i.e. not using any TCP/IP stack).
		</info>
		<info type="keyword" value="technology">
			<keyword value="Communication"/>
		</info>
		<build type="c-source" value="macb_example.c"/>
		<device-support value="uc3a"/>
		<device-support value="uc3c"/>
		<require idref="avr32.drivers.gpio"/>
		<require idref="avr32.drivers.intc"/>
		<require idref="avr32.drivers.macb"/>
		<require idref="avr32.drivers.pm"/>
		<require idref="avr32.utils.print_funcs"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
