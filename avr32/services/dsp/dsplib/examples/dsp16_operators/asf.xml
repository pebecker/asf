<asf xmlversion="1.0">
	<module type="application" id="avr32.services.dsplib.example.dsp16_operators" caption="DSPLIB - 16-bit fixed-point operators example">
		<info type="description" value="summary">
		Example application. It demonstrates how to manipulate 16-bit fixed-point numbers by using the operators function set provided in the DSP library.
		</info>
		<info type="keyword" value="technology">
			<keyword value="Digital Signal Processing"/>
		</info>
		<build type="c-source" value="operators_example.c"/>
		<device-support value="uc3"/>
		<require idref="avr32.drivers.pm"/>
		<require idref="avr32.drivers.cpu.cycle_counter"/>
		<require idref="avr32.services.dsplib.operators"/>
		<info type="gui-flag" value="move-to-root"/>
	</module>
</asf>
