<?xml version="1.0" encoding="utf-8"?>
<asf xmlversion="1.0">
	<select-by-device id="thirdparty.cmsis" caption="CMSIS thirdparty sources">
		<module type="library" id="thirdparty.cmsis#0" caption="CMSIS thirdparty sources">
			<info type="description" value="summary">
			The Cortex Microcontroller Software Interface Standard (CMSIS) provides a single standard across all
Cortex-Mx processor series vendors. It enables code re-use and code sharing across software projects
and reduces time-to-market for new embedded applications.
			</info>
			<build type="include-path" value="Include/" toolchain="armgcc"/>
			<build type="include-path" value="Lib/GCC/" toolchain="armgcc"/>
			<build type="library" subtype="user" value="Lib/GCC/libarm_cortexM3l_math.a" toolchain="armgcc"/>
			<build type="library" subtype="compiler" value="libm" toolchain="armgcc"/>
			<build type="define" name="ARM_MATH_CM3" value="true"/>
			<build type="header-file" value="Include/core_cm3.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cmFunc.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cmInstr.h" toolchain="armgcc"/>
			<device-support value="sam3"/>
			<build type="distribute" subtype="file" value="CMSIS END USER LICENCE AGREEMENT.pdf"/>
			<build type="distribute" subtype="file" value="README.txt"/>
			<build type="distribute" subtype="license" value="license.txt"/>
			<info type="gui-flag" value="hidden"/>
		</module>
		<module type="library" id="thirdparty.cmsis#1" caption="CMSIS thirdparty sources">
			<info type="description" value="summary">
			The Cortex Microcontroller Software Interface Standard (CMSIS) provides a single standard across all
Cortex-Mx processor series vendors. It enables code re-use and code sharing across software projects
and reduces time-to-market for new embedded applications.
			</info>
			<build type="include-path" value="Include/" toolchain="armgcc"/>
			<build type="include-path" value="Lib/GCC/" toolchain="armgcc"/>
			<build type="library" subtype="user" value="Lib/GCC/libarm_cortexM4l_math.a" toolchain="armgcc"/>
			<build type="library" subtype="compiler" value="libm" toolchain="armgcc"/>
			<build type="define" name="ARM_MATH_CM4" value="true"/>
			<build type="header-file" value="Include/core_cm4.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cm4_simd.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/arm_math.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cmFunc.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cmInstr.h" toolchain="armgcc"/>
			<device-support value="sam4s"/>
			<device-support value="sam4l"/>
			<require idref="thirdparty.cmsis.license"/>
			<info type="gui-flag" value="hidden"/>
		</module>
		<module type="library" id="thirdparty.cmsis#2" caption="CMSIS thirdparty sources">
			<info type="description" value="summary">
			The Cortex Microcontroller Software Interface Standard (CMSIS) provides a single standard across all
Cortex-Mx processor series vendors. It enables code re-use and code sharing across software projects
and reduces time-to-market for new embedded applications.
			</info>
			<build type="include-path" value="Include/" toolchain="armgcc"/>
			<build type="include-path" value="Lib/GCC/" toolchain="armgcc"/>
			<build type="library" subtype="compiler" value="libm" toolchain="armgcc"/>
			<build type="define" name="ARM_MATH_CM4" value="true"/>
			<build type="header-file" value="Include/core_cm4.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cm4_simd.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/arm_math.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cmFunc.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cmInstr.h" toolchain="armgcc"/>
			<require idref="sam.utils.fpu_used"/>
			<device-support value="sam4e"/>
			<require idref="thirdparty.cmsis.license"/>
			<info type="gui-flag" value="hidden"/>
		</module>
		<module type="library" id="thirdparty.cmsis#3" caption="CMSIS thirdparty sources">
			<info type="description" value="summary">
			The Cortex Microcontroller Software Interface Standard (CMSIS) provides a single standard across all
Cortex-Mx processor series vendors. It enables code re-use and code sharing across software projects
and reduces time-to-market for new embedded applications.
			</info>
			<build type="include-path" value="Include/" toolchain="armgcc"/>
			<build type="include-path" value="Lib/GCC/" toolchain="armgcc"/>
			<build type="library" subtype="user" value="Lib/GCC/libarm_cortexM0l_math.a" toolchain="armgcc"/>
			<build type="define" name="ARM_MATH_CM0" value="true"/>
			<build type="header-file" value="Include/core_cm0plus.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/arm_math.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cmFunc.h" toolchain="armgcc"/>
			<build type="header-file" value="Include/core_cmInstr.h" toolchain="armgcc"/>
			<device-support value="sam0"/>
			<require idref="thirdparty.cmsis.license"/>
			<info type="gui-flag" value="hidden"/>
		</module>
	</select-by-device>

	<select-by-config id="sam.utils.fpu_used" name="config.compiler.armgcc.fpu_used" caption="SAM FPU usage" default="yes">
		<module type="meta" id="sam.utils.fpu_used#no" caption="SAM with FPU but not used">
			<build type="library" subtype="user" value="Lib/GCC/libarm_cortexM4l_math.a" toolchain="armgcc"/>
		</module>
		<module type="meta" id="sam.utils.fpu_used#yes" caption="SAM with FPU used">
			<build type="library" subtype="user" value="Lib/GCC/libarm_cortexM4lf_math.a" toolchain="armgcc"/>
			<build type="build-config" subtype="assembler-flags" value="-mfloat-abi=softfp" toolchain="armgcc"/>
			<build type="build-config" subtype="c-compiler-flags" value="-mfloat-abi=softfp" toolchain="armgcc"/>
			<build type="build-config" subtype="c++-compiler-flags" value="-mfloat-abi=softfp" toolchain="armgcc"/>
		</module>
	</select-by-config>

	<module type="meta" id="thirdparty.cmsis.license" caption="CMSIS license">
		<build type="distribute" subtype="file" value="CMSIS END USER LICENCE AGREEMENT.pdf"/>
		<build type="distribute" subtype="file" value="README.txt"/>
		<build type="distribute" subtype="license" value="license.txt"/>
	</module>
</asf>
