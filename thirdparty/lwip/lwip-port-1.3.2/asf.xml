			<require idref="sam.drivers.pmc"/>
			<require idref="sam.drivers.rstc"/>
			<require idref="sam.components.ethernet_phy.dm9161a"/>
			<generator value="doxygen-module" default-mcu="unspecified-sam3x"/>
		</module>
