			<require idref="sam.drivers.pmc"/>
			<require idref="sam.components.ethernet_phy.ksz8051mnl"/>
			<generator value="doxygen-module" default-mcu="unspecified-sam4e"/>
		</module>
	</select-by-config>
</asf>
