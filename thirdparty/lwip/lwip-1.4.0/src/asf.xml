			<build type="header-file" subtype="api" value="include/ipv4/lwip/ip_frag.h"/>
			<build type="include-path" value="include/ipv4"/>
			<device-support value="uc3"/>
			<device-support value="sam3x"/>
			<device-support value="sam4e"/>
		</module>
		<module type="meta" id="config.thirdparty.lwip.140.ipv#6" caption="IPv6 support in lwIP v140">
			<info type="description" value="summary">
			lwIP configuration to use IPv6. Note that IPv6 is still in a very experimental state, and that no real support is actually done.
			</info>
