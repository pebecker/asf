	<select-by-config id="config.thirdparty.lwip.140.ipv" name="config.thirdparty.lwip.140.ipv" caption="IPv4 versus IPv6 configuration of lwIP v140" default="4">
		<module type="meta" id="config.thirdparty.lwip.140.ipv#4" caption="IPv4 support in lwIP v140">
			<info type="description" value="summary">
			Default lwIP configuration to use IPv4.
			</info>
			<build type="c-source" value="core/ipv4/autoip.c"/>
			<build type="c-source" value="core/ipv4/icmp.c"/>
			<build type="c-source" value="core/ipv4/igmp.c"/>
			<build type="c-source" value="core/ipv4/inet.c"/>
			<build type="c-source" value="core/ipv4/inet_chksum.c"/>
