<asf xmlversion="1.0">
	<select-by-device id="sam.applications.getting-started" caption="Getting-Started Application">
		<module type="application" id="sam.applications.getting-started#0" caption="Getting-Started Application on SAM">
			<info type="description" value="summary">
				This demonstration program makes two LEDs on the board blinking at a fixed rate.
			</info>
			<build type="c-source" value="main.c"/>
			<build type="include-path" value="."/>
			<device-support value="sam3"/>
			<device-support value="sam4s"/>
			<device-support value="sam4e"/>
			<device-support value="sam4n"/>
			<device-support value="sam4c"/>
			<device-support value="samg"/>
			<require idref="sam.drivers.tc"/>
			<require idref="sam.drivers.pmc"/>
			<require idref="sam.drivers.pio"/>
			<require idref="sam.drivers.uart"/>
			<require idref="common.services.ioport"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
		<module type="application" id="sam.applications.getting-started#1" caption="Getting-Started Application on SAM4L">
			<info type="description" value="summary">
				The demonstration program makes the on-board LED blink at a fixed rate.
			</info>
			<build type="c-source" value="main_sam4l.c"/>
			<build type="include-path" value="."/>
			<device-support value="sam4l"/>
			<require idref="sam.drivers.tc"/>
			<require idref="sam.drivers.eic"/>
			<require idref="common.services.basic.clock"/>
			<require idref="common.utils.stdio.stdio_serial"/>
			<info type="gui-flag" value="move-to-root"/>
		</module>
	</select-by-device>
</asf>
